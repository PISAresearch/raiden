#!/Users/Ali/Documents/kings/raiden/raiden/venv-raiden/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'py-evm','console_scripts','trinity'
__requires__ = 'py-evm'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('py-evm', 'console_scripts', 'trinity')()
    )
